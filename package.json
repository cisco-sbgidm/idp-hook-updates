  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/memory-cache": "^0.2.0",
    "axios": "^0.19.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "memory-cache": "^0.2.0"
  },
  "devDependencies": {
    "@types/jasmine-data-provider": "^2.2.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.5.0",
    "aws-sdk": "^2.610.0",
    "husky": "^4.0.7",
    "jasmine-data-provider": "^2.2.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "supertest": "^4.0.2",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
