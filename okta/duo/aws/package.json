{
  "name": "okta-duo-aws",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "tsc": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist ; sed -i '' 's+../../../../++' `find dist -name \"*.js\"`",
    "clean": "rm -Rf dist",
    "test": "yarn clean; jest",
    "zip": "yarn clean ; yarn tsc ; yarn install --prod --pure-lockfile --modules-folder dist/node_modules ; (cd dist; zip -r -x \"*__tests__*\" --symlink idp-hook-updates .;)",
    "lint": "tslint -c ../../../tslint.json src/*.ts src/**/*.ts",
    "configure": "yarn tsc ; node ./dist/okta/duo/aws/scripts/Configure.js"
  },
  "devDependencies": {
    "tscpaths": "^0.0.9"
  }
}
